<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.nightscout.notifier</string>
        <key>ProgramArguments</key>
        <array>
           <string>/usr/bin/python</string>
           <string>/usr/local/bin/nsnotifier.py</string>
        </array>
        <key>RunAtLoad</key>
        <true/>
  
        <key>StandardErrorPath</key>
        <string>/tmp/local.nsnotifier.stderr</string>
         
        <key>StandardOutPath</key>
        <string>/tmp/local.nsnotifier.stdout</string>

        <key>StartInterval</key>
        <integer>10</integer>
        <key>KeepAlive</key>
        <true/>
    </dict>
</plist>
